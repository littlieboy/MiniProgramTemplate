/* components/wshh-tab-control/wshh-tab-control.wxss */
.tables-control {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #f6f6f6;
  padding-bottom:10rpx;
}

.tables-item {
  flex: 1;
  text-align: center;
}

.active {
  color: red;
}

.active text {
  padding: 16rpx 16rpx;
  border-bottom: 6rpx solid red;
}
