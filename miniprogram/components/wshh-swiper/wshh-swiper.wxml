<!--components/wshh-swiper/wshh-swiper.wxml-->
<swiper class='swiper' circular autoplay indicator-dots interval='{{interval}}' duration='{{duration}}' indicator-active-color="#56abe4">
  <block wx:for='{{list}}' wx:key='index'>
    <swiper-item class='swiper-item'>
      <image src='{{item}}' mode="widthFix" />
    </swiper-item>
  </block>
</swiper>
